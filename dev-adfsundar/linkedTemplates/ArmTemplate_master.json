{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "dev-adfsundar"
		},
		"dest_ls_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'dest_ls'"
		},
		"source_ls_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'source_ls'"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "dev-adfsundar_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"dest_ls_connectionString": {
						"value": "[parameters('dest_ls_connectionString')]"
					},
					"source_ls_connectionString": {
						"value": "[parameters('source_ls_connectionString')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}